{
  "name": "server",
  "version": "1.0.0",
  "description": "Server for SEPER app",
  "main": "app.js",
  "proxy": "https://seper-mern-app.herokuapp.com",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "cd frontend && npm run build",
    "install-frontend": "cd frontend && npm install",
    "heroku-postbuild": "npm run install-frontend && npm run build",
    "server": "nodemon app.js",
    "develop": "concurrently --kill-others-on-fail \"npm run server\" \"npm run start --prefix frontend\"",
    "start": "concurrently --kill-others-on-fail \"npm run server\" \"npm run start --prefix frontend\""
  },
  "author": "Team 505",
  "license": "ISC",
  "dependencies": {
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@mui/icons-material": "^5.0.1",
    "@mui/material": "^5.0.1",
    "@types/mongoose": "^5.11.97",
    "axios": "^0.21.4",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^6.2.1",
    "config": "^3.3.6",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "mongoose": "^6.0.7",
    "nodemon": "^2.0.12",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.16.0",
    "react-router-dom": "^5.3.0",
    "react-scripts": "^4.0.3",
    "react-table": "^7.7.0",
    "styled-components": "^5.3.1",
    "validation": "0.0.1"
  }
}
